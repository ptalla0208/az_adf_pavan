{
	"name": "1_pl_copy_azsql_tbl_to_adls_snappy_parquet_files",
	"properties": {
		"activities": [
			{
				"name": "Copy data from az sql tbl to adlsg2 parquet files",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_azsql_general_use_tbl",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": {
								"value": "@pipeline().parameters.pl_pram_src_schemaName",
								"type": "Expression"
							},
							"tableName": {
								"value": "@pipeline().parameters.pl_param_src_tableName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_azadlsg2_general_use_snappy_parquet",
						"type": "DatasetReference",
						"parameters": {
							"directryName": {
								"value": "@pipeline().parameters.pl_param_src_tableName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pl_pram_src_schemaName": {
				"type": "string"
			},
			"pl_param_src_tableName": {
				"type": "string"
			}
		},
		"annotations": []
	}
}